{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "highlight",
    "patterns": [{ "include": "#a" }],
    "repository": {
        "a": {
            "match": "(?=hel(l))((hel)lo)",
            "name": "comment",
            "captures": {
                "1": { "name": "string" },
                "2": { "name": "keyword" },
                "3": { "name": "variable.parameter"}
            }
        }
    },
    "scopeName": "source.highlight"
}
