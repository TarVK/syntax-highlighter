{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "highlight",
	"patterns": [
		{
			"include": "#keywords"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.highlight.$1",
				"match": "\\b(if|while|for|return)\\b"
			}]
		}
	},
	"scopeName": "source.highlight"
}